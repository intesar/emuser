<dwr>
  <allow>
      
    <create creator="new" javascript="JContactsAjaxService">
      <param name="class" value="com.bia.imec.service.ajax.ContactsAjaxService"/>
    </create>
    
    <create creator="new" javascript="JEmailAjaxService">
      <param name="class" value="com.bia.imec.service.ajax.EmailAjaxService"/>
    </create>
    
    <create creator="new" javascript="JUserAjaxService">
      <param name="class" value="com.bia.imec.service.ajax.UserAjaxService"/>
    </create>
    
    
    
    <convert match="com.bia.imec.dto.AuthoritiesDto" converter="bean"/>
    <convert match="com.bia.imec.dto.ContactsDto" converter="bean"/>
    <convert match="com.bia.imec.dto.EmailsDto" converter="bean"/>
    <convert match="com.bia.imec.dto.MailServersDto" converter="bean"/>
    <convert match="com.bia.imec.dto.UserDto" converter="bean"/>
    
    <convert converter="bean" match="com.bia.imec.dto.*"/>
  </allow>
</dwr>