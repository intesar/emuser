<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="QuestionBankPU" transaction-type="RESOURCE_LOCAL">
    <provider>oracle.toplink.essentials.PersistenceProvider</provider>
    <class>com.cisco.questionbank.entity.Question</class>
    <class>com.cisco.questionbank.entity.QuestionComment</class>
    <class>com.cisco.questionbank.entity.QuestionDuplicate</class>
    <class>com.cisco.questionbank.entity.QuestionRank</class>
    <class>com.cisco.questionbank.entity.QuestionTag</class>
    <class>com.cisco.questionbank.entity.UserBookmarks</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      
      <property name="toplink.jdbc.user" value="root"/>
      <property name="toplink.jdbc.password" value="Abbh1234"/>
      <property name="toplink.jdbc.url" value="jdbc:mysql://localhost:3306/question_bank"/>
      <property name="toplink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      
    </properties>
  </persistence-unit>
</persistence>
