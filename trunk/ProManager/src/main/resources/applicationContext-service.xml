<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
		  "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
    
    
    <!--
     ##########################################################################
                            Services Declerations Begin
     ##########################################################################
    -->

    <!-- Example Services Decleration


    <bean id="personServiceImpl" class="com.company.projectname.service.impl.PersonServiceImpl" >
        <property name="personDao" ref="personDao"/>
    </bean>
    -->
    <bean id="usersServiceImpl" class="com.bizintelapps.promanager.service.impl.UsersServiceImpl" >
        <property name="usersDao" ref="usersDaoImpl" />
        <property name="usersConverter" ref="usersConverter" />
        <property name="usersValidator" ref="usersValidator" />
    </bean>
    
    
    <!-- converters -->
    <bean id="usersConverter" class="com.bizintelapps.promanager.service.converters.UsersConverter" />
    <!-- validators -->
    <bean id="usersValidator" class="com.bizintelapps.promanager.service.validator.UsersValidator" />
</beans>